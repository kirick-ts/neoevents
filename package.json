{
	"name": "neoevents",
	"version": "0.2.1-dev",
	"description": "",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.cjs"
		}
	},
	"engines": {
		"node": ">=16"
	},
	"devDependencies": {
		"@kirick/eslint-config": "latest",
		"@types/bun": "^1.2.12",
		"@types/node": "^22.15.13",
		"eslint": "9.10.0",
		"publint": "^0.3.12",
		"tsd": "^0.32.0",
		"tsdown": "^0.10.0",
		"typescript": "5.8.3",
		"unplugin-unused": "^0.4.4",
		"vitest": "3.1.3"
	},
	"scripts": {
		"build": "tsdown src/main.ts --publint --unused --dts --format esm --format cjs && rm dist/main.d.cts",
		"check": "bun run lint && bun run build && bun run test",
		"lint": "eslint . && tsc --skipLibCheck --noemit",
		"test": "tsd && bun test --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kirick-ts/neoevents.git"
	},
	"keywords": [],
	"author": "Daniil Kirichenko (https://twitter.com/kirickme)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kirick-ts/neoevents/issues"
	},
	"homepage": "https://github.com/kirick-ts/neoevents#readme"
}
